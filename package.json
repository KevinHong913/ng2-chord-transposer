{
  "name": "ng2-chord-transpose",
  "description": "Angular2 Chord Transpose Component",
  "version": "1.1.2",
  "scripts": {
    "tslint": "ng lint",
    "test": "karma start",
    "transpile": "ngc",
    "package": "rollup -c",
    "postcss": "postcss src/**/*.css --use autoprefixer  -d dist/bundles",
    "minify": "uglifyjs dist/bundles/ng2ChordTranspose.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/ng2ChordTranspose.umd.min.js",
    "build": "npm run transpile && npm run package && npm run minify && npm run readme",
    "readme": "cp README.md dist",
    "clean:src": "find . -name '*.ngfactory.ts' -delete && find . -name '*.ngsummary.json' -delete && find . -name '*.ngstyle.ts' -delete"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "angular",
    "angular2",
    "transpose"
  ],
  "author": "Kevin Hong",
  "license": "MIT",
  "main": "bundles/index.umd.js",
  "module": "index.js",
  "typings": "index.d.ts",
  "homepage": "https://github.com/KevinHong913/ng2-chord-transposer",
  "peerDependencies": {
    "@angular/core": "^4.1.0",
    "@angular/common": "^4.1.0"
  },
  "devDependencies": {
    "@angular/common": "^4.1.0",
    "@angular/compiler": "^4.1.0",
    "@angular/compiler-cli": "^4.1.0",
    "@angular/core": "^4.1.0",
    "@angular/animations": "^4.1.0",
    "@angular/platform-browser": "^4.1.0",
    "@angular/platform-browser-dynamic": "^4.1.0",
    "@angular/platform-server": "^4.1.0",
    "@types/jasmine": "2.5.37",
    "@types/node": "6.0.46",
    "@types/selenium-webdriver": "^3.0.3",
    "core-js": "2.4.1",
    "cz-conventional-changelog": "1.2.0",
    "istanbul-instrumenter-loader": "0.2.0",
    "jasmine-core": "2.5.2",
    "karma": "1.3.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "1.1.1",
    "karma-firefox-launcher": "1.0.0",
    "karma-jasmine": "1.0.2",
    "karma-mocha-reporter": "^2.1.0",
    "karma-remap-coverage": "~0.1.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.1",
    "loader-utils": "~0.2.16",
    "reflect-metadata": "^0.1.8",
    "rxjs": "^5.0.3",
    "source-map-loader": "0.1.5",
    "ts-helpers": "1.1.2",
    "tslint": "~4.5.0",
    "tslint-loader": "3.3.0",
    "typescript": "2.1.5",
    "webpack": "2.2.0",
    "zone.js": "^0.8.9",
    "rollup": "^0.41.6",
    "postcss": "^5.2.17",
    "postcss-loader": "^1.3.3",
    "autoprefixer": "~6.7.7"
  }
}
